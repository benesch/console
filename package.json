{
  "name": "cloud-frontend",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fontsource/inter": "^4.5.0",
    "@frontegg/react": "^3.0.12",
    "@segment/analytics.js-core": "^4.1.11",
    "@segment/analytics.js-integration-segmentio": "^4.4.7",
    "@sentry/react": "^6.11.0",
    "@sentry/tracing": "^6.12.0",
    "@types/mini-css-extract-plugin": "^2.3.0",
    "css-loader": "^6.4.0",
    "date-fns": "^2.25.0",
    "downloadjs": "^1.4.7",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "mini-css-extract-plugin": "^2.2.0",
    "react": "^17.0.2",
    "react-calendly": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.3.0",
    "restful-react": "^15.9.0",
    "stream": "^0.0.2",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.14.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-rc.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/downloadjs": "1.4.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.4.13",
    "@types/pg": "^8.6.1",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.1",
    "@types/webpack-dev-server": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "extract-zip": "^2.0.1",
    "fork-ts-checker-webpack-plugin": "^6.2.13",
    "gavi": "^0.2.2",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.0.5",
    "miragejs": "^0.1.42",
    "pg": "^8.7.1",
    "prettier": "^2.3.2",
    "pyright": "^1.1.177",
    "ts-jest": "^27.0.5",
    "webpack-dev-server": "^4.2.0"
  },
  "scripts": {
    "start": "cd frontend && webpack serve --config=webpack.dev.ts",
    "build": "cd frontend && NODE_ENV=production webpack",
    "test:unit": "cd frontend && jest",
    "test:e2e": "cd e2e-tests && DEBUG=pw:api playwright test",
    "eslint": "eslint --ignore-path .gitignore --max-warnings 0 .",
    "eslint:fix": "eslint --ignore-path .gitignore --fix .",
    "playwright": "playwright",
    "pyright": "pyright",
    "gavi": "gavi",
    "gen:api": "restful-react import --file api/schema.yml --output frontend/src/api/api.tsx --transformer frontend/api-transformer.js && eslint --fix frontend/src/api/api.tsx"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "prettier",
      "simple-import-sort",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error",
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars-experimental": "error"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
