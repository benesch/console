{
  "name": "cloud-console-frontend",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^4.1.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@segment/analytics.js-core": "^4.1.11",
    "@segment/analytics.js-integration-segmentio": "^4.4.7",
    "@sentry/react": "^6.8.0",
    "@sentry/tracing": "^6.10.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/downloadjs": "1.4.2",
    "@types/node": "^16.4.10",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.2.4",
    "downloadjs": "^1.4.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "extract-zip": "^2.0.1",
    "fomantic-ui-less": "^2.8.8",
    "fork-ts-checker-webpack-plugin": "^6.2.13",
    "formik": "^2.2.9",
    "jest": "^27.0.5",
    "jest-extended": "^0.11.5",
    "jest-puppeteer": "^5.0.4",
    "less": "^3.13.1",
    "less-loader": "^10.0.1",
    "mini-css-extract-plugin": "^2.2.0",
    "pg": "^8.7.1",
    "prettier": "^2.3.2",
    "puppeteer": "^10.1.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-password-strength-bar": "^0.3.5",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.2.0",
    "react-useinterval": "^1.0.2",
    "restful-react": "^15.7.0",
    "semantic-ui-react": "^2.0.3",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "webpack serve --config=webpack.dev.js",
    "build": "NODE_ENV=production webpack",
    "test:unit": "cd src && jest",
    "test:integration": "cd integration-tests && jest",
    "lint": "eslint --max-warnings 0 src",
    "lint:fix": "eslint --fix src",
    "gen:api": "restful-react import --file ../api/schema.yml --output src/api.tsx --transformer src/api-transformer.js && eslint --fix src/api.tsx"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars-experimental": "error"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
