{
  "name": "cloud-console-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@fullstory/browser": "^1.4.9",
    "@sentry/react": "^6.2.4",
    "@sentry/tracing": "^6.2.4",
    "amazon-cognito-identity-js": "^4.3.3",
    "aws-amplify": "^3.3.14",
    "aws-amplify-react": "^4.2.17",
    "downloadjs": "^1.4.7",
    "formik": "^2.2.6",
    "graphql": "^15.4.0",
    "graphql-playground-react": "^1.7.27",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-password-strength-bar": "^0.3.2",
    "react-router-dom": "^5.2.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "yup": "^0.32.8"
  },
  "scripts": {
    "start": "webpack serve --config=webpack.dev.js",
    "build": "webpack",
    "test": "jest",
    "lint": "eslint --max-warnings 0 src",
    "lint:fix": "eslint --fix src"
  },
  "engines": {
    "node": "14.x"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars-experimental": "error"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@aws-amplify/auth": "^3.4.16",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^5.0.2",
    "@testing-library/user-event": "^7.2.1",
    "@types/downloadjs": "1.4.0",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
