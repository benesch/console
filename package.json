{
  "name": "materialize-console",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.13.0"
  },
  "scripts": {
    "start": "webpack serve --config=webpack.dev.ts",
    "build": "NODE_ENV=production webpack --config=webpack.prod.ts",
    "build:analyze": "BUNDLE_ANALYZE=1 yarn build",
    "test:unit": "jest 'test.ts?(x)'",
    "test:e2e": "cd e2e-tests && DEBUG=pw:api playwright test",
    "eslint": "eslint --ignore-path .gitignore --max-warnings 0 .",
    "eslint:fix": "eslint --ignore-path .gitignore --fix .",
    "playwright": "playwright",
    "typecheck": "tsc",
    "gen:api": "CONSOLE_DIR=$PWD && cd ../cloud/api && openapi-typescript '*.yml' --output $CONSOLE_DIR/src/api/schemas/",
    "gen:sourcemap": "NODE_ENV=production SOURCE_MAPS=true GIT_SHA=$SENTRY_RELEASE LAUNCH_DARKLY_KEY=dummy-key webpack --config=webpack.prod.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.14",
    "@chakra-ui/modal": "^2.2.9",
    "@chakra-ui/popover": "^2.1.8",
    "@chakra-ui/react": "^2.4.9",
    "@chakra-ui/toast": "^4.0.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.8.1",
    "@fontsource/inter": "^4.5.14",
    "@fontsource/roboto-mono": "^4.5.8",
    "@frontegg/react": "^5.0.18",
    "@melloware/csp-webpack-plugin": "^6.0.4",
    "@segment/analytics-next": "^1.48.0",
    "@sentry/react": "^7.31.1",
    "@sentry/tracing": "^7.31.1",
    "@sentry/webpack-plugin": "^1.20.0",
    "@types/codemirror": "^5.60.5",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/react-table": "^7.7.12",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "codemirror": "^5.65.11",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "date-fns": "^2.29.3",
    "fast-deep-equal": "^3.1.3",
    "framer-motion": "^5.6.0",
    "launchdarkly-react-client-sdk": "^3.0.3",
    "mini-css-extract-plugin": "^2.7.2",
    "openapi-typescript-fetch": "^1.1.3",
    "react": "^18.2.0",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.6.1",
    "react-select": "^5.7.0",
    "react-table": "^7.8.0",
    "recharts": "^2.3.2",
    "recoil": "^0.7.6",
    "stream": "^0.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.5",
    "@frontegg/client": "^4.2.1",
    "@playwright/test": "^1.31.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-loader": "^9.1.0",
    "cacheable-lookup": "6.0.4",
    "core-js": "^3.26.1",
    "debug": "^4.3.4",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "extract-zip": "^2.0.1",
    "html-webpack-plugin": "^5.5.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.0.5",
    "jest-launchdarkly-mock": "^2.0.3",
    "miragejs": "^0.1.47",
    "msw": "^1.0.1",
    "openapi-typescript": "^6.1.0",
    "pg": "^8.8.0",
    "prettier": "^2.8.3",
    "react-refresh": "^0.11.0",
    "ts-jest": "^27.1.0",
    "typescript-eslint-language-service": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "prettier",
      "import",
      "simple-import-sort",
      "@typescript-eslint",
      "react"
    ],
    "ignorePatterns": [
      "src/api/schemas",
      "vendor"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "prettier/prettier": "error",
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error",
      "@typescript-eslint/no-shadow": [
        "error"
      ],
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "args": "none",
          "argsIgnorePattern": "^_",
          "destructuredArrayIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "react-hooks/exhaustive-deps": "error",
      "react/display-name": "off",
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function"
        }
      ],
      "react/prop-types": "off",
      "react/jsx-curly-brace-presence": [
        1,
        {
          "props": "never",
          "children": "never",
          "propElementValues": "always"
        }
      ]
    }
  },
  "browserslist": [
    "defaults"
  ],
  "volta": {
    "node": "18.15.0",
    "yarn": "1.22.17"
  }
}
