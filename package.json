{
  "name": "cloud-frontend",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "^1.0.17",
    "@chakra-ui/modal": "^1.10.10",
    "@chakra-ui/popover": "^1.11.6",
    "@chakra-ui/react": "^1.8.7",
    "@chakra-ui/toast": "^1.5.7",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/inter": "^4.5.7",
    "@frontegg/react": "^4.0.12",
    "@frontegg/react-auth": "^4.0.12",
    "@frontegg/react-core": "^4.0.12",
    "@segment/analytics-next": "^1.35.1",
    "@sentry/react": "^6.19.4",
    "@sentry/tracing": "^6.19.4",
    "@sentry/webpack-plugin": "^1.18.8",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/semver": "^7.3.9",
    "css-loader": "^6.5.1",
    "d3-scale-chromatic": "^3.0.0",
    "date-fns": "^2.28.0",
    "dayzed": "^3.2.2",
    "downloadjs": "^1.4.7",
    "formik": "^2.2.9",
    "framer-motion": "^5.6.0",
    "mini-css-extract-plugin": "^2.5.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-router-dom": "^5.3.0",
    "recoil": "^0.5.2",
    "restful-react": "^15.9.4",
    "semver": "^7.3.5",
    "stream": "^0.0.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "unstated-next": "^1.1.0",
    "victory": "^36.1.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.17.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/downloadjs": "1.4.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/pg": "^8.6.3",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.16.0",
    "concurrently": "7.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "extract-zip": "^2.0.1",
    "fork-ts-checker-webpack-plugin": "^7.2.2",
    "gavi": "^0.2.2",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.0.5",
    "miragejs": "^0.1.43",
    "pg": "^8.7.1",
    "prettier": "^2.5.1",
    "pyright": "1.1.221",
    "react-refresh": "^0.11.0",
    "ts-jest": "^27.1.0",
    "typescript-eslint-language-service": "^5.0.0",
    "webpack-dev-server": "^4.8.1",
    "cacheable-lookup": "6.0.4"
  },
  "scripts": {
    "start": "cd frontend && webpack serve --config=webpack.dev.ts",
    "build": "cd frontend && NODE_ENV=production webpack",
    "test:unit": "cd frontend && jest",
    "test:e2e": "cd e2e-tests && DEBUG=pw:api playwright test --workers=1",
    "eslint": "eslint --ignore-path .gitignore --max-warnings 0 .",
    "eslint:fix": "eslint --ignore-path .gitignore --fix .",
    "playwright": "playwright",
    "pyright": "pyright",
    "gavi": "gavi",
    "gen:api": "restful-react import --config frontend/restful-react.config.js && eslint --fix frontend/src/api/*.tsx",
    "gen:sourcemap": "cd frontend && NODE_ENV=production SOURCE_MAPS=true webpack",
    "dev:mock:prometheus": "python bin/prometheus-mock-server.py",
    "concurrently": "concurrently"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "prettier",
      "import",
      "simple-import-sort",
      "@typescript-eslint",
      "react"
    ],
    "rules": {
      "prettier/prettier": "error",
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error",
      "import/prefer-default-export": "error",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "react/display-name": "off",
      "react/function-component-definition": [
        2,
        {
          "namedComponents": "arrow-function"
        }
      ],
      "react/prop-types": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ]
  },
  "settings": {
    "react": {
      "version": "detect",
      "formComponents": [
        "Formik"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "14.19.0",
    "yarn": "1.22.17"
  }
}
